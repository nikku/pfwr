{
  "name": "pfwr",
  "version": "0.8.0",
  "description": "The last markdown to HTML slide show generator you need",
  "module": "dist/index.js",
  "bin": "./bin/cli.js",
  "type": "module",
  "scripts": {
    "all": "run-s lint bundle test",
    "bundle": "rollup -c",
    "dev": "chokidar --initial \"browser/*.*\" \"dist/*.*\" \"example/*.md\" --command \"npm start\"",
    "start": "node bin/cli.js example/presentation.md example/presentation.html",
    "version": "./tasks/bump-examples",
    "lint": "eslint .",
    "test": "node bin/cli.js example/presentation.md example/presentation.html",
    "prepare": "npm run bundle"
  },
  "author": "Nico Rehwaldt<https://github.com/nikku>",
  "license": "MIT",
  "keywords": [
    "markdown-to-html",
    "markdown",
    "presentation",
    "cli"
  ],
  "files": [
    "dist",
    "bin"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "chokidar-cli": "^3.0.0",
    "eslint": "^9.22.0",
    "eslint-plugin-bpmn-io": "^2.2.0",
    "js-yaml": "^4.1.0",
    "mdast-comment-marker": "^3.0.0",
    "npm-run-all2": "^7.0.2",
    "rehype-document": "^7.0.3",
    "rehype-format": "^5.0.1",
    "rehype-meta": "^4.0.1",
    "rehype-stringify": "^10.0.1",
    "rehype-wrap": "^1.0.10",
    "remark-breaks": "^4.0.0",
    "remark-emoji": "^5.0.1",
    "remark-external-links": "^9.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.1",
    "rollup": "^4.35.0",
    "rollup-plugin-string": "^3.0.0",
    "twemoji": "^14.0.2",
    "unified": "^11.0.5",
    "unist-util-visit-parents": "^6.0.1",
    "vfile": "^6.0.3"
  },
  "engines": {
    "node": ">= 16"
  },
  "dependencies": {
    "mri": "^1.1.6",
    "opener": "^1.5.2"
  }
}
